
public class Seperate_characters_and_numbers_of_string {

	public static void main(String[] args) {
		String rv="Hello123??@#";
		String chars="";
		String digits="";
		String special="";
		for(int i=0;i<rv.length();i++)
		{
			if(Character.isDigit(rv.charAt(i)))
				
			{
				digits = digits+rv.charAt(i);
				
			}
			
		}
	}

}
